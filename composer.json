{
    "name": "biskuit/biskuit",
    "type": "project",
    "description": "The Biskuit CMS",
    "keywords": ["cms", "biskuit"],
    "homepage": "https://biskuit.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Bis[ku]it Team",
            "email": "info@biskuit.org"
        }
    ],
    "require": {
        "php": ">=7.2",
        "composer/composer": "~1.9.1",
        "doctrine/annotations": "~1.8.0",
        "doctrine/dbal": "~2.10.0",
        "paragonie/random-lib": "~2.0.0",
        "maximebf/debugbar": "~1.15",
        "monolog/monolog": "~1.25",
        "nikic/php-parser": "~4.3.0",
        "psr/log": "~1.1.2",
        "swiftmailer/swiftmailer": "~6.2.3",
        "symfony/debug": "~4.3",
        "symfony/finder": "~4.3",
        "symfony/http-foundation": "~4.3",
        "symfony/routing": "~4.3",
        "symfony/stopwatch": "~4.3",
        "symfony/templating": "~4.3",
        "symfony/translation": "~4.3",
        "twig/twig": "^2.12.2"
    },
    "require-dev": {
        "ext-dom": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-xml": "*",
        "ext-xmlwriter": "*",
        "phpunit/phpunit": "~8.3"
    },
    "autoload": {
        "psr-4": {
            "Biskuit\\": "app/modules/application/src",
            "Biskuit\\Installer\\": "app/installer/src",
            "Biskuit\\System\\": "app/system/src",
            "Biskuit\\Config\\": "app/modules/config/src",
            "Biskuit\\Console\\": "app/console/src",
            "Biskuit\\Migration\\": "app/modules/migration/src"
        }
    },
    "config": {
        "vendor-dir": "app/vendor"
    }
}
